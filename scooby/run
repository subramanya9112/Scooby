docker build -t game-server -f Dockerfile .
docker tag game-server gcr.io/scooby-game/game-server
docker push gcr.io/scooby-game/game-server

gcloud run deploy --image gcr.io/scooby-game/game-server --platform managed --region us-central1 --port 80 --allow-unauthenticated


docker build -t client -f Dockerfile .
docker tag client gcr.io/scooby-game/client
docker push gcr.io/scooby-game/client

gcloud run deploy --image gcr.io/scooby-game/client --platform managed --region us-central1 --port 80 --allow-unauthenticated


